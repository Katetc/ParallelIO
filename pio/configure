ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS EXPLICIT_FPP DEBUG MPI_INC MPI_LIB FPP FPPFLAGS MACHDEFS FC MPIF90 FFLAGS ALLCFLAGS CC MPICC OPT REAL8 BIT64 ENDIAN MCT_PATH MODINC INCLUDEPATH AR ARFLAGS RM AWK PNETCDF_PATH NETCDF_PATH MEMMON_PATH STACKMON_PATH EXITHANDLER_PATH SIZEOF_CHAR SIZEOF_INT SIZEOF_FLOAT SIZEOF_DOUBLE SIZEOF_VOID_P CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT MPILIBS CPP EGREP PIO_PATH PDEFS PDEFSF PINCS LDLIBS F90 F90FLAGS F90SUFFIX ac_ct_F90 PKG_CONFIG ac_pt_PKG_CONFIG NETCDF4_CFLAGS NETCDF4_LIBS HDF5DIR ZLIBDIR SZLIBDIR LIBOBJS LTLIBOBJS'
