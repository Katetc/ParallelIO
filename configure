  for ac_arg in $ac_fc_v_output; do
     case $ac_arg in
        [\\/]*.a | ?:[\\/]*.a | -[lLRu]*)
          ac_cv_prog_fc_v=$ac_verb
          break 2 ;;
     esac
  done
