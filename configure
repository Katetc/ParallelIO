ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS DEBUG MPI_INC MPI_LIB FPP FPPFLAGS MACHDEFS FC MPIFC FFLAGS ALLCFLAGS CC MPICC OPT REAL8 BIT64 ENDIAN MCT_PATH MODINC INCLUDEPATH AR ARFLAGS RM AWK PNETCDF_PATH NETCDF_PATH MEMMON_PATH STACKMON_PATH EXITHANDLER_PATH SIZEOF_CHAR SIZEOF_INT SIZEOF_FLOAT SIZEOF_DOUBLE SIZEOF_VOID_P CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT FCFLAGS ac_ct_FC FCFLAGS_F90 FCFLAGS_F FC_DEFINE FCLIBS MPILIBS CPP EGREP FC_MODEXT FC_MODINC ac_empty HDF5_LIBS HDF5_LDFLAGS HDF5_CPPFLAGS NETCDF_LIBS NETCDF_LDFLAGS NETCDF_CPPFLAGS NETCDF_FCFLAGS PKG_CONFIG PKG_CONFIG_PATH PKG_CONFIG_LIBDIR ac_pt_PKG_CONFIG NETCDF4_CFLAGS NETCDF4_LIBS HDF5DIR ZLIBDIR SZLIBDIR PIO_PATH CPPDEFS LDLIBS LIBOBJS LTLIBOBJS'
ac_subst_files=''

# Initialize some variables set by options.
ac_init_help=
